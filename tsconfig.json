{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "_components": ["components"],
        },
        "outDir": "./dist",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "alwaysStrict": true,
        "allowSyntheticDefaultImports": true,
    }
}